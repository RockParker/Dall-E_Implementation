<Page x:Class="Dall_E_Implementation.Pages.SettingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Dall_E_Implementation.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SettingPage">
    <StackPanel Name="spSettings" Background="#252530">
        <Label Name="lblTitle" Foreground="White">Settings</Label>
        <TextBox Name="tbApiKey"      
                GotKeyboardFocus="tbAPIKey_GotKeyboardFocus" 
                GotMouseCapture="tbAPIKey_GotMouseCapture"  
                 Background="Transparent"
                 Foreground="LightGray"
                 TextAlignment="Center">API Key Here</TextBox>
                                                                                                 
        <Button Name="btnSave" Click="btnSave_Click" >Save</Button>
        <Button Name="btnBack" Click="btnBack_Click">Back</Button>

    </StackPanel>
</Page>
